.container.container-edit-profile
  .row
    .col.s12
      - if @company.type == TypeCompany::COMPANY
        = render 'companies/edit_form_company'

      - else
        = render 'companies/edit_form_team_company'
      
      .col
        = link_to root_path, class: 'btn-large waves-effect waves-light btn-cancel-company-profile' do
          | Cancel
        
      = link_to "Cancel Suscription", cancel_account_path(@company), class:'btn-large waves-effect waves-light red btn right'

= javascript_include_tag 'address_autocomplete.js'